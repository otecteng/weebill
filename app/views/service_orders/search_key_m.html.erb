<header data-role="header" class="">
      <h1>
        安装查询
      </h1>
</header>
<div class="main" data-role="content">
  <div class="main-content">
    <div class="form-content">
      <div id="dlg-search">
        <div class="field  " data-api-code="field_3">
        <div class="control-group ">
          <div class="field_content">
            <div class="help-block"><%=@message if @message%></div>
          </div>
        </div>
        </div>

      <%unless @result then%>
      <%=form_tag '/service_orders/search_key_m',:method=>'post' do%>
      <input id="" name="worker_id" size="30" type="tel" value="<%=params[:worker]%>">
      <input id="" name="user_id" size="30" type="tel" value="<%=params[:user]%>">
      <div class="field" data-api-code="field_3">
        <div class="control-group ">
          <div class="field_content">
              <div class="help-block">请输入安装预约号:</div>
            <div class="controls">
              <input id="entry_field_3" name="tid" size="30" type="tel" value="">
            </div>
          </div>
        </div>
      </div>
      <div class="field submit-field">
        <input id="btn_search" name="commit" type="submit" value="验证预约号">
      </div>
      <%end%>


      <%end%>
      </div>

      <div id="dlg-message">
        <div class="field  " data-api-code="field_3">
        <div class="control-group ">
          <div class="field_content">
            <div class="help-block" id="msg"></div>
          </div>
        </div>
        </div>
      </div>

</div>
</div>
</div>
</div>
<script type="text/javascript">
  $(function(){
    $('#dlg-report').hide();
    $('#dlg-report-confirm').hide();
  })
</script>


