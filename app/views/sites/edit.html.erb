<%= form_for(@site) do |f| %>
  <div class="field">
    名称：<%= f.text_field :name %><br>
  </div><p>
  <div class="field">
    地址：<%= f.text_field :address %><br>
  </div><p>

  <div class="field">
  	省市：
  </div><p>
  <div class="field">
    坐标：<%= f.text_field :location %><br>
  </div><p>
  <div class="field">
    星级：<%= f.text_field :star %><br>
  </div><p>

  <div class="actions">
    <%= submit_tag '保存',class: "btn btn-primary",style:"margin-top: 30px;width: 100px;"%>
  </div>
<% end %>

